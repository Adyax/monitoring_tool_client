/**
 * Implements hook_install().
 */
function monitoring_tool_client_install() {
  variable_set(MONITORING_TOOL_CLIENT_SETTINGS, array(
    'project_id' => '',
    'secure_token' => '',
    'webhook' => TRUE,
    'report_interval' => 0,
    'skip_updates' => array(),
    'skip_drupal_database_update' => FALSE,
  ));
}
